                <ImageView
                    android:layout_width="60dip"
                    android:layout_height="50dip"
                    android:layout_gravity="center_vertical"
                    android:paddingLeft="10dip"
                    android:contentDescription="@string/UKFlagDesc"
                    android:src="@drawable/ukflag" />

