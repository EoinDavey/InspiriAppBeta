                android:paddingLeft="5dp"
                android:background="@drawable/customborder" >

                <Button
                    android:id="@+id/ContactButtonSamUK"
                    android:layout_width="60dp"
                    android:layout_height="80dp"
                    android:background="@drawable/callicon"
